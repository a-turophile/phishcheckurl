{
    "name": "first extension",
    "version": "1.0.0",
    "description": "just learniing stuff",
    "manifest_version": 2,
    "background":{
        "scripts":["background.js"],
        "persistent":false

    },
    "content_scripts": [ {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      } ],

    "permissions":[
        "bookmarks", "tabs"
    ]
}